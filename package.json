{
  "name": "content-machine",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:e2e": "node scripts/dev-e2e.mjs",
    "build": "npm run check:llm-calls && next build",
    "build:skip-check": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint . --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check:llm-calls": "node scripts/check-llm-calls.js",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:seed": "npx supabase db reset --seed-only"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.1",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.7",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}
