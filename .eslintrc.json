{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-console": ["error", {
      "allow": ["warn", "error"]
    }]
  },
  "overrides": [
    {
      "files": ["scripts/**/*.{js,mjs,ts}"],
      "rules": {
        "no-console": "off"
      }
    },
    {
      "files": ["lib/log.ts"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
